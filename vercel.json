{
  "version": 3,
  "build": {
    "env": {
      "NODE_ENV": "production"
    },
    "distDir": "dist"
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}